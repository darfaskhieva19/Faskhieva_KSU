<Application x:Class="KSU.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:KSU"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>               
                <ResourceDictionary Source="/Themes/SummerTheme.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Themes/AutumnTheme.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Themes/WinterTheme.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Themes/SpringTheme.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
